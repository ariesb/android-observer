{
	"name": "android-observer",
	"version": "1.0.0",
	"description": "Simple android video recorder",
	"keywords": [
		"Android",
		"observer",
		"capture",
		"video"
	],
	"bugs": {},
	"license": "CC0-1.0",
	"author": "GitHub",
	"main": "main.js",
	"repository": {
		"type": "git",
		"url": "g"
	},
	"scripts": {
		"start": "electron .",
		"pack:osx": "electron-packager . $npm_package_productName --overwrite --out=dist/osx --platform=darwin --arch=x64 --icon=./images/icon.icns",
		"pack:win32": "electron-packager . $npm_package_productName --out=dist/win --platform=win32 --arch=ia32",
		"pack:win64": "electron-packager . $npm_package_productName --out=dist/win --platform=win32 --arch=x64 --version=0.36.2 app-version=1.0 --icon=assets/build/win/icon.ico",
		"build": "npm run pack:osx && npm run pack:win32 && npm run pack:win64",
		"test": "echo test complete."
	},
	"dependencies": {
		"every-moment": "0.0.1",
		"split-lines": "^1.1.0"
	},
	"devDependencies": {
		"electron-packager": "^7.3.0",
		"electron-prebuilt": "^1.2.0"
	}
}
